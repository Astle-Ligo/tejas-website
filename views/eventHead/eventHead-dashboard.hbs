<h2>Event Head Dashboard</h2>
<p>Welcome, {{eventHeadUser.Name}}</p>

<h3>Your Events:</h3>
<div class="eventwise-box">
    {{#each events}}
    <div class="alert alert-primary" role="alert">
        <a href="/eventHead/registrations/{{this._id}}" class="event-link">{{this.eventName}}</a>
    </div>
    {{else}}
    <p>No events found.</p>
    {{/each}}
</div>